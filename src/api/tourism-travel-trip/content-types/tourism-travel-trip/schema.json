{
  "kind": "collectionType",
  "collectionName": "tourism_travel_trips",
  "info": {
    "singularName": "tourism-travel-trip",
    "pluralName": "tourism-travel-trips",
    "displayName": "Tourism Travel Trip"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "coverimage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Description_in_detail": {
      "type": "richtext"
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    },
    "hashtags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::hashtag.hashtag",
      "mappedBy": "triptags"
    },
    "category": {
      "type": "string",
      "required": true
    },
    "countries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::country.country",
      "mappedBy": "tourism_travel_trips"
    },
    "states": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::state.state",
      "mappedBy": "tourism_travel_trips"
    },
    "cities": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::city.city",
      "mappedBy": "tourism_travel_trips"
    },
    "publishedat": {
      "type": "date"
    },
    "short_description": {
      "type": "text"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author",
      "inversedBy": "tourism_travel_trips"
    },
    "Tags": {
      "type": "string"
    },
    "destination": {
      "type": "string"
    },
    "schema": {
      "type": "string"
    }
  }
}
